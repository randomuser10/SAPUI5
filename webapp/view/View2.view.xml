<mvc:View controllerName="projectapp.projectmds.controller.View2"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m" xmlns:l="sap.ui.layout">
    <Page id="page1" title="Employee Personal" showNavButton="true" navButtonPress="goBack" >
        <content>
            <ObjectHeader id="obj1" title="Samay" number="10101010101010101010101010" numberUnit="INR"
            backgroundDesign="Solid">
                <attributes>
                    <ObjectAttribute id="objatt1" title="Employee ID" text="1010"></ObjectAttribute>
                    <ObjectAttribute id="objatt2" title="Employee Designation" text="Spider-Man"></ObjectAttribute>                  
                </attributes>
                <statuses>
                    <ObjectStatus id="ObjSt1" title="Status" text="Permanant" state="Success"></ObjectStatus>
                </statuses>            
            </ObjectHeader>
            <IconTabBar id="icontab1">
                <items>
                    <IconTabFilter id="icon1" icon="sap-icon://information"></IconTabFilter>
                    <IconTabFilter id="icon2" icon="sap-icon://notes"></IconTabFilter>
                </items>
            </IconTabBar>
            <Panel id="idPanel2" headerText="Employee Personal Details" expandable="true">
                <l:Grid id="idGrid2" defaultSpan="XL3 L6 M6 S12">
                    <Label id="idLblEid" text="Employee ID"></Label>
                    <Input id="idInp2" editable="false"></Input>
                    <Label id="idLblEname" text="Employee Name"></Label>
                    <Input id="idInp3" editable="false"></Input>
                </l:Grid>
                
            
            </Panel>
        </content>
    </Page>
</mvc:View>